{
  "name": "hacker-tab-extension",
  "version": "0.0.0",
  "description": "Browser extension for hackers",
  "license": "MIT",
  "keywords": [
    "react",
    "github",
    "browser extension",
    "chrome extension",
    "firefox extension"
  ],
  "main": "src/index.js",
  "dependencies": {
    "@atlaskit/button": "^13.0.8",
    "@atlaskit/flag": "^12.0.8",
    "@atlaskit/icon": "^18.0.5",
    "@atlaskit/select": "^9.1.7",
    "@atlaskit/spinner": "^12.0.3",
    "@atlaskit/theme": "^9.1.1",
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.12",
    "@huchenme/github-trending": "^1.8.0",
    "append-query": "^2.0.1",
    "lodash": "^4.17.11",
    "polished": "^3.4.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-event-listener": "^0.6.6",
    "react-popper": "^1.3.3",
    "react-scripts": "^3.0.1",
    "react-spring": "^8.0.25",
    "react-use": "^9.7.1",
    "styled-components": "^3.4.10"
  },
  "scripts": {
    "start": "react-scripts start",
    "storybook": "start-storybook -p 6006",
    "test": "react-scripts test --env=jsdom",
    "prebuild": "rimraf build",
    "build": "INLINE_RUNTIME_CHUNK=false react-scripts build",
    "prezip": "rimraf *.zip",
    "zip": "npm-run-all zip:*",
    "zip:build": "zip -rj build.zip build",
    "zip:src": "zip -r src.zip src package.json README.md public",
    "prebuild-storybook": "rimraf storybook-static",
    "build-storybook": "build-storybook",
    "release": "npm-run-all build zip"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-use-before-define": "off"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@reach/component-component": "^0.1.3",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-centered": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/addon-notes": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "@testing-library/react": "^8.0.2",
    "jest-dom": "^3.5.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3"
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true
  }
}
